    "test": "craco test",
    "lint": "eslint src",
    "coverage": "npm test -- --coverage",
    "eject": "craco eject",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/App.js",
      "!src/config/i18n.js",
      "!src/**/index.js",

      "!src/terra-front/**/stories/**/*.js",
      "!src/terra-front/**/*.stories.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 25,
        "branches": 25,
        "lines": 25,
        "functions": 25
      },
      "./src/terra-front": {
        "statements": 83,
        "branches": 85,
        "lines": 83,
        "functions": 78
      }
    },
    "moduleNameMapper": {
      "^@terralego/core/(.*)": "<rootDir>/src/terra-front/$1"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
